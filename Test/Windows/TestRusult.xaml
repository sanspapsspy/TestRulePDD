<Window x:Class="Test.Windows.TestRusult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test.Windows"
        mc:Ignorable="d"
        Title="TestRusult" Height="600" Width="800">
    <Grid>
        <StackPanel>
            <DataGrid x:Name="Datas" d:ItemsSource="{d:SampleData ItemCount=10}" Background="#FFB37D7D" SelectionChanged="DataGrid_SelectionChanged" />
        </StackPanel>
    </Grid>
</Window>
